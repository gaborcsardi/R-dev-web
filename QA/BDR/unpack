foreach p (../contrib/*.tar.gz)
set pkgname=`echo "${p}" | sed -e 's+^../contrib/++' -e 's/_.*//'`
echo $pkgname
tar zxf $p
touch -r $p $pkgname.in
end
foreach p (../2.5.0/*.tar.gz)
set pkgname=`echo "${p}" | sed -e 's+^../2.5.0/++' -e 's/_.*//'`
echo $pkgname
rm -rf $pkgname
tar zxf $p
touch -r $p $pkgname.in
end
