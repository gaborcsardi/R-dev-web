foreach p (../contrib/*.tar.gz)
set pkgname=`echo "${p}" | sed -e 's+^../contrib/++' -e 's/_.*//'`
echo $pkgname
tar zxf $p
touch $pkgname.in
end
rm -rf foreign
foreach p (../2.3.0/*.tar.gz)
set pkgname=`echo "${p}" | sed -e 's+^../2.3.0/++' -e 's/_.*//'`
echo $pkgname
tar zxf $p
touch $pkgname.in
end
