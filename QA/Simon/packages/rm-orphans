#!/bin/sh

LOC="$1"
SRC="$2"
if [ -z "$LOC" ]; then
    LOC=tiger-universal/bin/2.6
fi
if [ -z "$SRC" ]; then
    SRC=CRAN/src/contrib
fi

ORPH=`echo "${LOC}"|sed 's:/bin/:/orphan/:'`
mkdir -p "$ORPH"
for i in `ls $LOC/|sed s:_.*::`; do
    if ls $SRC/$i* >/dev/null 2>/dev/null; then
	echo "OK  $i"
    else
	echo "*** $i"
	mv "$LOC/$i"* "$ORPH"
    fi
done
