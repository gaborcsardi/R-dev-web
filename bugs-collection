//Hide status bar script - by Blackbox Hosting
//Credit must stay intact for use

function hidestatus(){
window.status=3D''
return true
}

if (document.layers)
document.captureEvents(Event.MOUSEOVER | Event.MOUSEOUT)

document.onmouseover=3Dhidestatus
document.onmouseout=3Dhidestatus
</script>

