#! /bin/csh
cd ~/R
./update
cd ~/R/R-patched
make
cd ~/R/R-devel
make
cd ~/R/R-clang
make
cd R/packages
Rscript unavailable.R
env DISPLAY=:5 NOAWT=1 RMPI_TYPE=OPENMPI RGL_USE_NULL=true Rscript updates.R
env DISPLAY=:5 NOAWT=1 RMPI_TYPE=OPENMPI RGL_USE_NULL=true ~/R/R-devel/bin/Rscript updates.R
cd tests
make unpack
./script
cd ../tests-clang
make unpack
./script
cd ../tests-devel
make unpack
./script
